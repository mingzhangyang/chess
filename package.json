{
  "name": "chess",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "npm run build && wrangler dev",
    "start": "npm run worker:dev",
    "legacy:dev": "tsx server.ts",
    "legacy:start": "node server.ts",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "rm -rf dist",
    "lint": "tsc --noEmit",
    "test": "node --import tsx --test tests/**/*.test.ts",
    "worker:dev": "npm run build && wrangler dev",
    "worker:typegen": "wrangler types",
    "worker:deploy": "npm run build && wrangler deploy"
  },
  "dependencies": {
    "@tailwindcss/vite": "^4.1.14",
    "@vitejs/plugin-react": "^5.0.4",
    "chess.js": "^1.4.0",
    "express": "^4.21.2",
    "lucide-react": "^0.546.0",
    "react": "^19.0.0",
    "react-chessboard": "^5.10.0",
    "react-dom": "^19.0.0",
    "socket.io": "^4.8.3",
    "vite": "^6.2.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^22.14.0",
    "tailwindcss": "^4.1.14",
    "tsx": "^4.21.0",
    "typescript": "~5.8.2",
    "wrangler": "^4.50.0"
  }
}
